<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:stretchColumns="1"
    android:background="#000000">

    <TableRow>
        <TextView
            android:layout_column="1"
            android:text="Open..."
            android:padding="3dip"
            android:textColor="#FF909090"
            />
        <TextView
            android:text="Ctrl-O"
            android:gravity="end"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
    </TableRow>
    <TableRow>
        <TextView
            android:layout_column="1"
            android:text="Save..."
            android:padding="3dip"
            android:textColor="#FF909090"
            />
        <TextView
            android:text="Ctrl-S"
            android:gravity="end"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
    </TableRow>
    <TableRow>
        <TextView
            android:layout_column="1"
            android:text="Save as..."
            android:padding="3dip"
            android:textColor="#FF909090"
            />
        <TextView
            android:text="Ctrl-Shift-S"
            android:gravity="end"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
    </TableRow>
    <View
        android:layout_height="2dip"
        android:background="#FF909090"
        />
    <TableRow>
        <TextView
            android:text="X"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
        <TextView
            android:text="Import..."
            android:padding="3dip"
            android:textColor="#FF909090"
            />
    </TableRow>
    <TableRow>
        <TextView
            android:text="X"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
        <TextView
            android:text="Export..."
            android:padding="3dip"
            android:textColor="#FF909090"
            />
        <TextView
            android:text="Ctrl-E"
            android:gravity="end"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
    </TableRow>
    <View
        android:layout_height="2dip"
        android:background="#FF909090"
        />
    <TableRow>
        <TextView
            android:layout_column="1"
            android:text="Quit"
            android:padding="3dip"
            android:textColor="#FF909090"
            />
    </TableRow>


</TableLayout>